{
  "name": "biomathcore-platform",
  "private": true,
  "version": "1.0.0",
  "description": "BioMath Core - Advanced Health Analytics Platform",
  "author": "BioMath Core Team",
  "license": "UNLICENSED",
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "vercel-build": "npm run build",
    "start": "vite preview",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:smoke": "node tests/api.smoke.mjs",
    "test:load": "k6 run tests/load.k6.js",
    "format": "prettier --write .",
    "check": "npm run lint && npm run typecheck",
    "validate": "npm run check && npm run test",
    "db:types": "npx supabase gen types typescript --project-id <YOUR_PROJECT_ID> > src/types/database.ts",
    "prebuild": "node scripts/gen-version.mjs"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^16.5.4",
    "stripe": "^19.2.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/ui": "^4.0.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jsdom": "^27.0.1",
    "postcss": "^8.4.35",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vitest": "^4.0.1"
  }
}
